{
  "name": "typescript-node-starter",
  "version": "1.0.0",
  "description": "typescript nodejs starter",
  "author": "Tristan Teufel",
  "homepage": "teufel-it.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/firsttris/typescript-node-starter.git"
  },
  "main": "dist/index",
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.13",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.3.0",
    "typescript": "^2.4.1",
    "webpack": "^3.3.0"
  },
  "scripts": {
    "dev-server": "nodemon",
    "tsc-build": "tsc",
    "tsc-watch": "tsc --watch",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "webpack-build": "webpack",
    "docker-build": "docker-compose build",
    "docker-up": "docker-compose up",
    "update-dependencies": "yarn upgrade-interactive"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    }
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "ts-node": "^3.2.0"
  }
}
